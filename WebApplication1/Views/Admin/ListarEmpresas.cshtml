@model IEnumerable<WebApplication1.Models.Empresa>

@{
    ViewBag.Title = "ListarEmpresas";
}

<h2>ListarEmpresas</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
       
    </tr>
}

</table>
